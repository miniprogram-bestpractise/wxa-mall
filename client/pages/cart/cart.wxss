@import "../../templates/toast/toast.wxss";

/**cart.wxss**/
.cartlist{
    width: 100%;
}
.cartbox {
    width: 100%;
    border-top: 1px solid #e5e5e5;
    background: #fff;
    margin-bottom: 10px;
}
.supplier{
    padding: 0 13px;
    height: 41px;
    line-height: 41px;
    background: #f8f8f8;
    font-size: 14px;
    color: #000;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.i-default{
    flex: 1;
    height: 43px;
    color: #000;
    font-size: 14px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: flex;
    flex-direction: row;
    align-items: center;    
}
.i-radio {
    margin: 0 5px 0 0;
    width: 18px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.i-checked{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.i-action{
    width: 50px;
    height: 43px;
    font-size: 13px;
    color: #444;
    text-align: right;
}
.i-edit{
    width: 100%;
    height: 100%;
}
.i-finish{
    width: 100%;
    height: 100%;
}
.goodsbox {
    padding: 0 13px;
}
.goods{
    width: 100%;
    height: 109px;
    display: flex;
    flex-direction: row;
}
.radiobox {
    width: 28px;
    height: 109px;
}
.box-radio {
    margin-top: 46px;
    width: 18px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.box-checked{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;    
}
.imgbox {
    margin: 11px 9px 0 0;
    width: 85px;
    height: 85px;
    border: 1px solid #e5e5e5;
    overflow: hidden;
}
.good-link{
    width: 85px;
    height: 85px;
}
.djimg{
    width: 85px;
    height: 85px;    
}
.goodinfo {
    flex: 1;
    margin-top: 11px;
    width: 170px;
    overflow: hidden;
}
.goodname {
    min-width: 170px;
    width: auto;
    max-width: 264px;
    font-size: 13px;
    color: #000;
    line-height: 16px;
    height: 32px;
    overflow: hidden;
}
.sizebox{
    margin-top: 1px;
    margin-bottom: 1px;
    font-size: 12px;
    color: #7c7c7c;
    height: 23px;
    line-height: 23px;
    position: relative;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden; 
    display: flex;
    flex-direction: row;
    align-items: center;   
}
.box-color{
    padding-right: 6px;
}
.box-size{
    padding-right: 6px;    
}
.i-arrdon{
    width: 11px;
    height: 6px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABYAAAAMBAMAAACU11D1AAAAFVBMVEUAAAC8vLy8vLy8vLy8vLy8vLy8vLzm4COtAAAABnRSTlMADMNvbo8KXP/9AAAAR0lEQVQI1yXJwQnAIABD0ZQuULpBLx3Bs+AMOoBo9h9ByM8pvK+xxMrWcM29vfV6wf50/a7h+SgBFgFOgAktTHCYABNgQg8fMSsQRm3agg0AAAAASUVORK5CYII=) no-repeat;
    background-size: 100%;
    margin-right: 5px;
}
.sizebox-edit {
    background: #f1f1f1;
    min-width: 170px;
    width: auto;
    max-width: 264px;
}
.benefit{
    font-size: 12px;
    color: #7c7c7c;
    height: 10px;
    line-height: 10px;    
}
.goodpri {
    margin-top: 3px;
    position: relative;
    height: 15px;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.good-price{
    font-size: 12px;
    color: #f7545f;
    flex: 1;
}
.price-red{
    font-size: 16px;
}
.good-num{
    width: 75px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAJYAAAAwBAMAAAABap4iAAAAElBMVEXl5eXx8fH////l5eWqqqq+vr6znYYQAAAAAXRSTlOWWYruFwAAAFFJREFUSMdjYDamFjBgMBTEAZRwAVwahEfNGjVr1CxSzRJxcRw1a9SsIWxWKBIgYBbMGARwHHlmDdZ4HDVr1KzBZ9ZgrbdHzRo1i35mUbPPBwCxGHNoypcwzwAAAABJRU5ErkJggg==) no-repeat;
    background-size: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;    
}
.numico{
    width: 25px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    font-size: 13px;
    color: #383838;    
}
.num-cur{
    width: 25px;
    height: 24px;
    line-height: 24px;
    text-align: center;
    font-size: 13px;
    color: #383838;
}

/*btnbox*/
.cart-btnbox {
    position: fixed;
    bottom: 0px;
    left: 0;
    width: 100%;
    height: 49px;
    background: #fff;
    border-bottom: 1px solid #e5e5e5;
    box-shadow: 0 -2px 10px #e5e5e5;
    z-index: 3;
}
.balance{
    width: 100%;   
    display: flex;
    flex-direction: row;    
}
.totalpri{
    flex: 1;
    padding: 10px 9px 0 13px;
    text-align: right;
}
.prinum{
    height: 14px;
    line-height: 14px;
    font-size: 15px;
    color: #383838;
}
.pritip{
    margin-top: 5px;
    font-size: 12px;
    color: #7c7c7c;
    text-align: right;
}
.cart-btn{
    width: 120px;
    height: 49px;
    line-height: 49px;
    text-align: center;
    background: #f7545f;
    font-size: 16px;
    color: #fff;
}
.del-check{
    margin-left: 13px;
    width: 50px;
    height: 49px;
    color: #444;
    font-size: 12px;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    line-height: 49px;
    flex: 1;
    display: flex;
    flex-direction: row;
    align-items: center;     
}
.del-checked{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;    
}
/*empty tip*/
.combox {
    margin: 0 auto;
    width: 220px;
    padding: 100px 0 50px;
    text-align: center;
    height: auto;
}


.combox .com-cart {
    margin: 0 auto;
    display: block;
    width: 58px;
    height: 55px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}

.combox .com-tips {
    margin: 10px auto 33px;
    width: 220px;
    min-height: 12px;
    height: auto;
    line-height: 16px;
    color: #7c7c7c;
    font-size: 13px;
}

.combox .btn-tips {
    margin: 0 auto;
    width: 135px;
    height: 37px;
    line-height: 37px;
    text-align: center;
    display: block;
    font-size: 15px;
    color: #f7545f;
    background: none;
    border: 1px solid #f7545f;
    border-radius: 5px;
}

