/**productlist.wxss**/
@import '../../templates/toast/toast.wxss';
@import '../../templates/cart/cart.wxss';
.navbar{
    width: 100%;
    height: 91px;
    background: #f1f1f1;
    transition: all .3s linear;
}
.searchbar {
    width: 100%;
    height: 49px;
    line-height: 49px;
    color: #fff;
    background: #f1f1f1;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.bar-class {
    margin-left: 10px;
    width: 23px;
    height: 21.5px;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACwAAAApBAMAAAC8Z9aBAAAAElBMVEUAAACbm5ubm5ubm5ubm5ubm5vbqqkTAAAABXRSTlMAvPMeH3sECDAAAAAuSURBVCjPYxgIYKgaigaChIHCoqEYIBAojFCMUI5btSGmchGGoQtGg2o0qJAAAN5yXeIFGNxaAAAAAElFTkSuQmCC) no-repeat;
    background-size: 100%;
}
.searchbox {
    font-size: 14px;
    text-align: center;
    padding: 0 10px;
    position: relative;
    flex: 1;
}
.iptbox {
    background: #fff;
    border-radius: 3px;
    width: 100%;
    height: 30px;
    line-height: 30px;
    border: 1px solid #dcdcdc;
}
.iptwrap {
    position: relative;
    padding: 0 34px;
    height: 30px;
    margin: 0 auto;
}
.comipt{
    width: 100%;
    height: 22px;
    line-height: 22px;
    color: #a0a0a0;
    margin: 4px 0;
    text-align: left;
    font-size: 14px;   
}
.iptwrap:after {
    content: "";
    position: absolute;
    left: 12px;
    top: 4px;
    width: 13.5px;
    height: 13.5px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
    display: block;
}
.search-del {
    position: absolute;
    right: 18px;
    top: 10px;
    width: 10.5px;
    height: 10.5px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
    text-indent: -9999em;
    display: none;
    z-index: 20;
}

/**all baby**/
.babytab {
  width: 100%;
  height: 40px;
  line-height: 40px;
  margin: 0 auto -10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
}
.babytabbox{
  flex: 1;
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #7c7c7c;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:center;
}
.curtabbox{
  color: #f7545f;
  font-size: 13px;
}
.ico-arr {
  display: block;
  margin: 4px 0 0 3px;
  width: 6.5px;
  height: 8.5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.ico-atop{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}
.ico-adown{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}

/**list**/
.listbox{
  width: 100%;
  border-top: 1px solid #e5e5e5;
  background: #f1f1f1;
  position: relative;
  padding-bottom: 50px;
}
.dj-tit{
  padding: 0 10px;
  border-bottom: 1rpx solid #e5e5e5;
  background: #fff;
  height: 44px;
  line-height: 44px;
  font-size: 15px;
  color: #383838;  
  display: block;
}
.djlist{
  width: 100%;
}
.djbox{
  float: left;
  margin: 0 0 1%;
  width: 49.5%;
  box-sizing: border-box;
  min-height: 240px;    
  position: relative;
  background: #fff;
}
.djbox:nth-child(odd){
  margin-right: 1%;
}
.dj-link{
  width: 100%;
  height: 100%;
}
.djmark{
  position: absolute;
  top: 0;
  left: 7px;
  z-index: 20;
  display: block;
  width: 22px;
  height: 26.5px;
  background-position: -135px -83px;
  font-size: 11px;
  color: #fff;
  text-align: center;
  line-height: 11px;
  padding-top: 2px;
  font-style: normal; 
}
.djimgbox {
  margin: 0 auto 12px;
  width: 100%;
  min-height: 150px;
  overflow: hidden;
}
.djimg {
  display: block;
  width: 100%;
  min-height: 150px;
}   
.djname {
  display: block;
  margin: 0 auto;
  margin-bottom: 11px;
  font-size: 13px;
  color: #383838;
  line-height: 16px;
  width: 88%;
  height: 32px;
  text-overflow: ellipsis;
  overflow: hidden;
}
.pricebox {
  margin: 0 auto;
  width: 88%;
  text-align: left;
  height: 32px;
  line-height: 14px;
}
.djpri {
  font-size: 14px;
  color: #f7545f;
}
.djoldpri {
  padding: 1px 0 0 5px;
  text-decoration: line-through;
  font-size: 10px;
  color: #aaa;  
}
.btn-cart {
  position: absolute;
  bottom: 12px;
  right: 7px;
  z-index: 10;
  display: block;
  width: 37px;
  height: 38px;
  text-indent: -9999em;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.btn-cart:hover{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}