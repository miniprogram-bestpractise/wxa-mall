@import '../../templates/toast/toast.wxss';
@import '../../templates/cart/cart.wxss';

/**index.wxss**/
.container{
  padding-bottom: 50px;
}
.tabbar{
  width: 100%;
  height: 60px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
  border-bottom: 1px solid #e5e5e5;
}
.tabbox{
  margin: 11px auto 0;
  height: 49px;
  flex: 1;
  align-items: center;
  box-sizing: border-box;
}
.curtab{
  border-bottom: 2px solid #f7545f;  
}
.tabico{
  margin: 0 auto;
  width: 24px;
  height: 23px;
  position: relative; 
}
.point-tip{
  position: absolute;
  top: 0;
  right: -1px;
  z-index: 2;
  width: 7.5px;
  height: 7.5px;
  border-radius: 50%;
  background: #ff3535;
}
.ico-recom{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}
.ico-baby{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.ico-new{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.ico-discount{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.ico-notice{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.cur-recom{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;  
}
.cur-baby{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.cur-new{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.cur-discount{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.cur-notice{
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
.tabtit{
  display: block;
  margin:3px auto 0;
  width: 100%;
  text-align: center;
  font-size: 12px;
  color: #383838;
}
.curtit{
  color: #f7545f;
}

/**swiper**/
.pagebox{
  width: 100%;
}
.page{
  width: 100%;
}
.adbox{
  width: 100%;
}
.slide-image{
  width: 100%;
  height: 141px;
}

/**list**/
.listbox{
  width: 100%;
  border-top: 1px solid #e5e5e5;
  margin-top: 10px;
  background: #f1f1f1;
  position: relative;
  display:block;
}
.listboxhide{
  width: 100%;
  border-top: 1px solid #e5e5e5;
  margin-top: 10px;
  background: #f1f1f1;
  position: relative;
  display: none;
}
.hide
{
  display: none;
}
.dj-tit{
  padding: 0 10px;
  border-bottom: 1rpx solid #e5e5e5;
  background: #fff;
  height: 44px;
  line-height: 44px;
  font-size: 15px;
  color: #383838;  
  display: block;
}
.djlist{
  width: 100%;
}
.djbox{
  float: left;
  margin: 0 0 1%;
  width: 49.5%;
  box-sizing: border-box;
  min-height: 240px;    
  position: relative;
  background: #fff;
}
.djbox:nth-child(odd){
  margin-right: 1%;
}
.dj-link{
  width: 100%;
  height: 100%;
}
.djmark{
  position: absolute;
  top: 0;
  left: 7px;
  z-index: 20;
  display: block;
  width: 22px;
  height: 26.5px;
  background-position: -135px -83px;
  font-size: 11px;
  color: #fff;
  text-align: center;
  line-height: 11px;
  padding-top: 2px;
  font-style: normal;  
}
.djimgbox {
  margin: 0 auto 12px;
  width: 100%;
  min-height: 150px;
  overflow: hidden;
}
.djimg {
  display: block;
  width: 100%;
  min-height: 150px;
}   
.djname {
  display: block;
  margin: 0 auto;
  margin-bottom: 11px;
  font-size: 13px;
  color: #383838;
  line-height: 16px;
  width: 88%;
  height: 32px;
  text-overflow: ellipsis;
  overflow: hidden;
}
.pricebox {
  margin: 0 auto;
  width: 88%;
  text-align: left;
  height: 32px;
  line-height: 14px;
}
.djpri {
  font-size: 14px;
  color: #f7545f;
}
.djoldpri {
  padding: 1px 0 0 5px;
  text-decoration: line-through;
  font-size: 10px;
  color: #aaa;  
}
.btn-cart {
  position: absolute;
  bottom: 12px;
  right: 7px;
  z-index: 10;
  display: block;
  width: 37px;
  height: 38px;
  text-indent: -9999em;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.btn-cart:hover{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}

/**all baby**/
.babytab {
  width: 100%;
  height: 40px;
  line-height: 40px;
  margin: 0 auto -10px;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #fff;
}
.babytabbox{
  flex: 1;
  height: 40px;
  line-height: 40px;
  text-align: center;
  color: #7c7c7c;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content:center;
}
.curtabbox{
  color: #f7545f;
}
.ico-arr {
  display: block;
  margin: 4px 0 0 3px;
  width: 6.5px;
  height: 8.5px;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.ico-atop{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}
.ico-adown{
  background:url(data:image/png;base64,) no-repeat;
  background-size:100%;
}

/**newtit**/
.newtit{
  position: relative;
  margin-top: 10px;
  width: 100%;
  height:12px;
}
.newdj-tit {
    position: absolute;
    top: -4px;
    left: 50%;
    margin-left: -29px;
    background: #f1f1f1;
    width: 58px;
    text-align: center;
    font-size: 13px;
    color: #7c7c7c;
}
.titline{
    margin: 0 auto;
    width: 90%;
    height: 5px;
    border: none;
    border-bottom: 1px solid #e5e5e5;
}

/**noticelist**/
.noticelist{
  width: 100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.noticebox{
  width: 92%;
  border-bottom: 1px solid #e5e5e5;
  height: 60px;
  text-align: left;
}
.noticebox:last-child{
  border-bottom: none;
}
.notice-link{
  width: 100%;
  height: 60px;
}
.notice-tit {
  display: block;
  margin: 0 auto;
  padding-top: 14px;
  width: 100%;
  height: 15px;
  font-size: 13px;
  color: #000;
}
.notice-time{
  margin: 4px auto;
  width: 100%;
  font-size: 10px;
  color: #7c7c7c;  
}